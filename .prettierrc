{
  "singleQuote": true,
  "trailingComma": "all",
  "importOrder": [
    "<BUILTIN_MODULES>",
    "^@/",
    "",
    "^[../]",
    "",
    "^[./]",
    "",
    "^[^.]*\\.scss$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins": [
    "typescript",
    "jsx",
    "decorators-legacy"
  ],
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ]
}